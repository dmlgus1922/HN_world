<body>
    <h1>카카오맵 API 연습용</h1>

    <!-- 지도가 들어갈 자리 -->
    <div id="map" style="width:80%;height:100vh;"></div>

    <!-- api 불러오기 -->
    <script type="text/javascript" src="https://dapi.kakao.com/v2/maps/sdk.js?appkey=e67b92f679778369a72327f7fcc1e7ce&libraries=services"></script>

    <script>
        var container = document.getElementById('map');
        var options = {
            center: new kakao.maps.LatLng(35.146561, 126.922227),
            level: 3
        };
        var map = new kakao.maps.Map(container, options);
    </script>
</body>

